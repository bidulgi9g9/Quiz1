    "while True:\n",
    "    a = list(map(int, input().split()))\n",
    "    if min(a) == max(a) == 0:\n",
    "        break\n",
    "    else:\n",
    "        if a.pop(a.index(max(a)))**2 == a[0]**2 + a[1]**2:\n",
    "            print('right')\n",
    "        else:\n",
    "            print('wrong')\n"
   ]
